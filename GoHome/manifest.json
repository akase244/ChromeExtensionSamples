{
    "manifest_version": 2,
    "name": "GoHome",
    "version": "0.0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "http://www.yahoo.co.jp/*"
            ],
            "js": ["script.js"]
        }
    ],
    "permissions": ["tabs"]
}
