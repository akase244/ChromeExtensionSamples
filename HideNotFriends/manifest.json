{
    "manifest_version": 2,
    "name": "HideNotFriends",
    "version": "0.0.1",
    "description": "Facebookで「知り合いかも」と表示される領域を削除する。",
    "content_scripts": [
        {
            "matches": ["https://www.facebook.com/*"],
            "js": ["jquery-1.11.1.min.js", "script.js"],
            "run_at": "document_end"
        }
    ]
}
